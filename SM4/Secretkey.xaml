<Window x:Class="SM4.Secretkey"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SM4"
        mc:Ignorable="d"
        Title="密钥输入" Height="227.557" Width="251.42" Closed="Window_Closed">
    <Grid>
        <Label x:Name="label" Content="输入密钥：" HorizontalAlignment="Left" Margin="24,43,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="94,49,0,0" VerticalAlignment="Top" Width="116"/>
        <Label x:Name="label1" Content="确认密钥：" HorizontalAlignment="Left" Margin="24,92,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.468,0.49"/>
        <PasswordBox x:Name="passwordBox1" HorizontalAlignment="Left" Margin="94,96,0,0" VerticalAlignment="Top" Width="116"/>
        <Button x:Name="button" Content="确定" HorizontalAlignment="Left" Margin="24,147,0,0" VerticalAlignment="Top" Width="82" Click="button_Click"/>
        <Button x:Name="button1" Content="取消" HorizontalAlignment="Left" Margin="128,147,0,0" VerticalAlignment="Top" Width="82" Click="button1_Click"/>

    </Grid>
</Window>
